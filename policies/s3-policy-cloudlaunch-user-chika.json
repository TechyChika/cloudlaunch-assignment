{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ListAllBuckets",
      "Effect": "Allow",
      "Action": "s3:ListAllMyBuckets",
      "Resource": "*"
    },
    {
      "Sid": "SiteBucketAccess",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetObject"
      ],
      "Resource": [
        "arn:aws:s3:::cloudlaunch-site-bucket-chika",
        "arn:aws:s3:::cloudlaunch-site-bucket-chika/*"
      ]
    },
    {
      "Sid": "PrivateBucketAccess",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetObject",
        "s3:PutObject"
      ],
      "Resource": [
        "arn:aws:s3:::cloudlaunch-private-bucket-chika",
        "arn:aws:s3:::cloudlaunch-private-bucket-chika/*"
      ]
    },
    {
      "Sid": "VisibleOnlyBucketList",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": "arn:aws:s3:::cloudlaunch-visible-only-bucket-chika"
    }
  ]
}